				<b>Brocker IP </b>
				<input required pattern="^([0-9]{1,3}\.){3}[0-9]{1,3}$" name="ip">
				<br>
				<br>
				<input type="submit" value="connect to brocker">
			</form>
			<form method="get" action="disconnect.cgi" name="numberform" id="fromID5" style="display: inline;">
					<input type="submit" value="disconnect from brocker">
			</form>
		</div>
		<br>

		<div class="border">
			<form method="get" action="enable.cgi" name="numberform" id="fromID5" style="display: inline;">
